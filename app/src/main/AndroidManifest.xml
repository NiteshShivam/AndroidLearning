<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nitesh.multitasking">
<uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Fifteen"></activity>
        <activity android:name=".Eight" />
        <activity android:name=".Seven" />
        <activity android:name=".Six" />
        <activity android:name=".Nine" />
        <activity android:name=".Ten" />
        <activity android:name=".Eleven" />
        <activity android:name=".Twelve" />
        <activity android:name=".Thirteen" />
        <activity android:name=".Fourteen" />
        <activity android:name=".Fifth" />
        <activity android:name=".Fourth" />
        <activity android:name=".Third" />
        <activity android:name=".Second" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>